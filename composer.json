{
  "name": "six-it/newsletter-bundle",
  "license": "GPL-3.0-or-later",
  "type": "pimcore-bundle",
  "description": "Pimcore Newsletter Bundle",
  "config": {
    "sort-packages": true,
    "preferred-install": {
      "six-it/pimcore-source": "source",
      "*": "dist"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "doctrine/dbal": "^3.6",
    "six-it/admin-ui-classic-bundle": "^1.0",
    "six-it/pimcore-source": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.12.15",
    "phpstan/phpstan-symfony": "^1.2.20"
  },
  "autoload": {
    "psr-4": {
      "Pimcore\\Bundle\\NewsletterBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Pimcore\\Bundle\\NewsletterBundle\\PimcoreNewsletterBundle"
      ]
    }
  }
}
